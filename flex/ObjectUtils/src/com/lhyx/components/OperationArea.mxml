<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			import mx.containers.ViewStack;
			
			import spark.components.Label;
			import spark.components.TextArea;

			
			private var _logLabel:Label;
			private var _logTextArea:TextArea;
			
			private var _viewStack:ViewStack;

			public function get logLabel():Label
			{
				return _logLabel;
			}

			public function set logLabel(value:Label):void
			{
				_logLabel = value;
			}

			public function get logTextArea():TextArea
			{
				return _logTextArea;
			}

			public function set logTextArea(value:TextArea):void
			{
				_logTextArea = value;
			}

			public function get viewStack():ViewStack
			{
				return _viewStack;
			}

			public function set viewStack(value:ViewStack):void
			{
				_viewStack = value;
			}

		]]>
	</fx:Script>
	
</s:Group>
